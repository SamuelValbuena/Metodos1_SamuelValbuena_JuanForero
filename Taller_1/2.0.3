    "def num_aureo(x):\n",
    "    l = Sucesion_fibonnaci(x)\n",
    "    la = []\n",
    "    for j in range(1, len(l)-1):            \n",
    "        d = l[j+1] / l[j]\n",
    "        la.append(d)\n",
    "    return la\n",
    "\n",
    "au = ((1 + ((5)**(1/2))) / 2)\n",
    "imagen_2 = plt.plot(num_aureo(20), color=\"b\", label='Estimación usando la serie')\n",
    "plt.axhline(au, linestyle=\"--\", color=\"r\", label='Numero Aureo')\n",
    "plt.xlabel('[Figura 2.0.3.3] Numero Aureo', labelpad=20)\n",
    "plt.grid()\n",
    "plt.legend()\n",
    "plt.show()"
