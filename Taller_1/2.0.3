   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def Sucesion_fibonnaci(p):\n",
    "    nu = np.linspace(2, p, p-2, dtype=int)\n",
    "    lista = []\n",
    "    i = 1\n",
    "    fo = 0\n",
    "    f1 = 1\n",
    "    lista.append(fo)\n",
    "    lista.append(f1)\n",
    "    for i in nu:\n",
    "        fn = fo + f1\n",
    "        fo = f1\n",
    "        f1 = fn\n",
    "        lista.append(fn)          \n",
    "    return lista\n",
    "print(\"Estos son los 20 primeros términos de la sucesión de Fibonacci: \")\n",
    "print(Sucesion_fibonnaci(20))\n"
   ]
  },
